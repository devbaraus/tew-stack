<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
<meta name="generator" content="{{ site.generator }}">
<title>{{ title }} | {{ site.short_name }}</title>
<meta name="title" content="{{ title }} | {{ site.short_name }}">
<meta name='description' content='{{ description | default(site.description) }}' />
<meta name='keywords' content='{{ keywords | default(site.keywords) }}' />
<meta name='author' content='{{ site.author }}'>
<meta name='robot' content='index, follow'>

<!-- Favicon -->
<link rel="icon" type="image/png" href="/img/favicon-16.png" sizes="16x16" />
<link rel="icon" type="image/png" href="/img/favicon-48.png" sizes="48x48" />
<link rel="icon" type="image/png" href="/img/favicon-192.png" sizes="192x192" />
<link rel="icon" type="image/png" href="/img/favicon-384.png" sizes="384x384" />
<link rel="icon" type="image/png" href="/img/favicon-512.png" sizes="512x512" />

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="{{ site.url }}">
<meta property="og:title" content="{{ title }} | {{ site.short_name }}">
<meta property="og:description" content="{{ description | default(site.description) }}">
<meta property="og:image" content="{{ oglarge | default(site.url + "/img/tew_large.png") }}">
<meta property="og:image:height" content="1200">
<meta property="og:image:width" content="628">

<!-- Open Graph / WhatsApp -->
<meta property="og:image" content="{{ ogsquared | default(site.url + "/img/tew_squared.png") }}">
<meta property="og:image:height" content="512">
<meta property="og:image:width" content="512">

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:url" content="{{ site.url }}">
<meta property="twitter:title" content="{{ title }} | {{ site.short_name }}">
<meta property="twitter:description" content="{{ description | default(site.description) }}">
<meta property="twitter:image" content="{{ oglarge | default(site.url + "/img/tew_large.png") }}">

<link rel="apple-touch-icon" href="/img/favicon-180.png">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="{{ site.theme_color }}" />
<meta name="apple-mobile-web-app-title" content="{{ title }} | {{ site.short_name }}" />

<meta name="theme-color" content="{{ site.theme_color }}">
<link rel='manifest' href='/manifest.json'>

<!-- Generated Bundle -->
<link rel="stylesheet" type="text/css" href="/css/main.css">
<script src="/js/main.js" defer></script>
